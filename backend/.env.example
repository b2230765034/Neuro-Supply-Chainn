# Backend environment example
# Copy to .env and fill your real values (do NOT commit secrets to Git)

HUGGINGFACE_API_TOKEN=your_huggingface_api_token_here
QWEN_MODEL=Qwen/Qwen2.5-7B-Instruct
QWEN_MAX_TOKENS=1000
QWEN_TEMPERATURE=0.7

# Sui / Move contract settings
CONTRACT_PACKAGE_ID=your_contract_package_id_here
CONTRACT_MODULE_NAME=supply_chain
SUI_RPC_URL=https://fullnode.testnet.sui.io:443
SUI_NETWORK=testnet

# Oracle key (if backend will sign transactions)
ORACLE_PRIVATE_KEY= # if you want the backend to sign, set this (hex)
ORACLE_ADDRESS= # optional

# Backend server
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000

# Logging
LOG_LEVEL=INFO
LOG_FILE=oracle.log
